      <pose frame=''>-5 7 0.0 0 -0 0</pose>
    </model>



    <model name='ground_plane_3'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>3 3</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
